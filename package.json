{
  "name": "fsbapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "servers": "npm-run-all --parallel server1 server2 server3 server4 server5 server7",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "server1": "dotnet run --project ../FSBWebApi/FSBApiGateway",
    "server2": "dotnet run --project ../FSBWebApi/LibraryModule/LibraryModule.Controllers",
    "server3": "dotnet run --project ../FSBWebApi/Client/Client.Controllers",
    "server4": "dotnet run --project ../FSBWebApi/Catalogs/Catalogs.Controllers",
    "server5": "dotnet run --project ../FSBWebApi/Users/User.Controllers",
    "server6": "dotnet run --project ../FSBWebApi/DashBoard/DashBoard.Controllers",
    "server7": "dotnet run --project ../FSBWebApi/Activity/Activity.Controllers"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.2",
    "@angular/cdk": "^14.1.2",
    "@angular/common": "^14.1.2",
    "@angular/compiler": "^14.1.2",
    "@angular/core": "^14.1.2",
    "@angular/forms": "^14.1.2",
    "@angular/material": "^14.1.2",
    "@angular/platform-browser": "^14.1.2",
    "@angular/platform-browser-dynamic": "^14.1.2",
    "@angular/router": "^14.1.2",
    "@azure/msal-angular": "^2.5.1",
    "@azure/msal-browser": "^2.32.1",
    "@material/layout-grid": "^14.0.0",
    "@microsoft/signalr": "^7.0.0",
    "@nebular/auth": "^10.0.0",
    "@nebular/eva-icons": "^9.0.3",
    "@nebular/security": "^9.1.0-rc.6",
    "@nebular/theme": "^9.0.3",
    "@ng-select/ng-select": "^9.0.2",
    "bootstrap": "^5.2.0",
    "eva-icons": "^1.1.3",
    "file-saver": "^2.0.5",
    "ngx-daterangepicker-material": "^6.0.4",
    "ngx-easy-table": "^15.3.0",
    "ngx-file-drop": "^14.0.1",
    "ngx-print": "^1.2.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
